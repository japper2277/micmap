{
  "name": "micmap-api",
  "version": "1.0.0",
  "description": "Simple API proxy for Google Sheets open mic data",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node scripts/seed-database.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:critical": "jest --testNamePattern='Critical Path'",
    "verify-env": "node scripts/verify-env.js",
    "smoke-test": "node scripts/smoke-test.js",
    "smoke-test:production": "node scripts/smoke-test.js https://micmap-api.herokuapp.com",
    "pre-deploy": "npm run verify-env && npm test"
  },
  "keywords": [
    "open-mic",
    "comedy",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^126.0.1",
    "gtfs-realtime-bindings": "^1.1.1",
    "ioredis": "^5.3.2",
    "mongoose": "^8.0.3"
  },
  "devDependencies": {
    "ioredis-mock": "^8.13.1",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.3.0",
    "nodemon": "^3.0.1",
    "supertest": "^7.1.4"
  }
}
